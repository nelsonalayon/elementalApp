{
  "kind": "collectionType",
  "collectionName": "apartments",
  "info": {
    "singularName": "apartment",
    "pluralName": "apartments",
    "displayName": "Apartment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "idapto": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "apartmentvalue": {
      "type": "biginteger"
    },
    "link": {
      "type": "string",
      "required": true
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment"
    },
    "deliverDate": {
      "type": "date"
    },
    "monthly": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "apartment"
    }
  }
}
